{
  "name": "gitsz",
  "version": "3.0.4",
  "description": "Git tags with higher level of security",
  "main": "lib/gitsz.js",
  "bin": {
    "gitsz": "bin/gitsz"
  },
  "scripts": {
    "prettier": "npx prettier --write . ",
    "eslint": "./node_modules/.bin/eslint .",
    "test": "npx tape test/*-test.js",
    "posttest": "tap --coverage-report=text-lcov | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sambacha/gitsz.git"
  },
  "keywords": [
    "git",
    "secure",
    "tag",
    "sha256",
    "gpg",
    "signature",
    "verify",
    "sign"
  ],
  "author": "sambacha  <sambacha@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sambacha/gitsz/issues"
  },
  "homepage": "https://github.com/sambacha/gitsz#readme",
  "dependencies": {
    "async": "^3.0.0",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "codecov": "3.8.3",
    "eslint": "7.21.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "tape": "5.3.1"
  }
}
