{
  "name": "gitsz",
  "version": "1.0.3",
  "description": "Git tags with higher level of security",
  "main": "lib/git-sz.js",
  "bin": {
    "git-sz": "bin/git-sz"
  },
  "scripts": {
    "prettier": "npx prettier --write . ",
    "eslint": "./node_modules/.bin/eslint .",
    "test": "npx tape test/*-test.js",
    "posttest": "tap --coverage-report=text-lcov | codecov"

  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sambacha/git-sz.git"
  },
