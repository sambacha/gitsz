  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sambacha/git-sz/issues"
  },
  "homepage": "https://github.com/sambacha/git-sz#readme",
  "dependencies": {
    "async": "^3.0.0",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "codecov": "^3.8.1",
    "eslint": "7.21.0",
    "rimraf": "3.0.2",
    "tape": "5.2.2"
  }
}
